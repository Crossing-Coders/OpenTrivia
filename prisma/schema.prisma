// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TriviaVenue {
  id           Int           @id @default(autoincrement())
  name         String        @unique
  email        String?
  phone        String?
  triviaEvents TriviaEvent[]
  address      String
  city         String
  state        String
  zipCode      Int
  url          String
}

model TriviaEvent {
  id            Int         @id @default(autoincrement())
  name          String
  restraunt     TriviaVenue @relation(fields: [triviaVenueId], references: [id])
  triviaVenueId Int
  starts_at     DateTime
  price         Int?
}
