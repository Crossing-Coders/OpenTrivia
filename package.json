{
  "name": "open-trivia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:db": "docker-compose up -d",
    "dev:next": "next dev",
    "dev": "npm-run-all -p dev:*",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "stop:db": "docker-compose down",
    "prisma:reseed": "NODE_ENV=development prisma migrate reset"
  },
  "dependencies": {
    "@prisma/client": "4.11.0",
    "@types/node": "^18.15.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "next": "13.2.3",
    "prisma": "^4.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
